{
  "name": "kdoc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "prisma db seed",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit",
    "check": "pnpm lint && pnpm type-check",
    "find-unused": "next-unused",
    "analyze-unused": "next-unused --json > unused-files.json && echo 'Analysis saved to unused-files.json'",
    "clean-unused:analyze": "pnpm run analyze-unused && tsx scripts/clean-unused.ts",
    "clean-unused:delete": "next-unused --deleteUnused"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "@prisma/client": "^6.13.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "konva": "^9.3.22",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.12",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-konva": "^19.0.7",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/glob": "^9.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "next-unused": "^0.0.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "next-unused": {
    "alias": {
      "shared/*": [
        "./shared/*"
      ],
      "entities/*": [
        "./entities/*"
      ],
      "features/*": [
        "./features/*"
      ],
      "widgets/*": [
        "./widgets/*"
      ]
    },
    "include": [
      "shared",
      "entities",
      "features",
      "widgets",
      "app"
    ],
    "exclude": [
      "node_modules",
      ".next",
      ".git",
      "prisma/migrations",
      "public",
      ".*\\.test\\.(ts|tsx|js|jsx)$",
      ".*\\.spec\\.(ts|tsx|js|jsx)$",
      ".*\\.d\\.ts$",
      ".*\\.config\\.(ts|tsx|js|jsx)$"
    ],
    "entrypoints": [
      "app",
      "middleware.ts"
    ],
    "debug": false
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
